/* pages/index/psy/index.wxss */
page{
  background: #F1F1F1;
}

.total-backcolor{
  background-color: #F1F1F1;
}

.image-view{
  display: flex;
  width: 750rpx;
  justify-content: center;
  align-items: center;
}

.image{
  width: 700rpx;
  border-radius: 10rpx;
}

.title-text{
  display: flex;
  font-weight: bold;
  margin-left: 10rpx;
}

.right_view{
  height: 42rpx;
  width: 8rpx;
  margin-left: 30rpx;
  background-color: #E64174;
}

.all-items{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 30rpx;
  width: 750rpx;
}

.item-view{
  width: 700rpx;
  margin-bottom: 30rpx;
  display: flex;
  flex-direction: column;
  background-color: #ffffff;
  align-items: center;
  justify-content: center;
  border-radius: 20rpx;
  padding-bottom: 30rpx;
  padding-top: 30rpx;
}

.item-head-name{
  display: flex;
  flex-direction: row;
  width: 700rpx;

}

.item-image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  border: solid;
  display: flex;
  margin-left: 30rpx;
}

